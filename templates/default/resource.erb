[template]
src = "<%= @src %>"
dest = "<%= @dest %>"
keys = [
<% @keys.each do |k| -%>
  "<%= k %>",
<% end -%>
]
<% if @prefix -%>
prefix = "<%= @prefix %>"
<% end -%>
<% if @owner -%>
owner = "<%= @owner %>"
<% end -%>
<% if @group -%>
group = "<%= @group %>"
<% end -%>
<% if @mode -%>
mode = "<%= @mode %>"
<% end -%>
<% if @reload_cmd -%>
reload_cmd = "<%= @reload_cmd %>"
<% end -%>
<% if @check_cmd -%>
check_cmd = "<%= @check_cmd %>"
<% end -%>
